<%= simple_form_for @vaccination_point_type, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :name %>
  <%= f.input :cod %>

  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                vaccination_point_types_path, :class => 'btn btn-default' %>
<% end %>
