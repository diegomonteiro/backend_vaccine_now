<%= simple_form_for @vaccination_point_type, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :name, :label => "Nome" %>
  <%= f.input :cod, :label => "Cód."  %>
  <%= f.input :icon_class, :label => "Ícone"  %>

  <%= f.button :submit,"Confirmar", :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                vaccination_point_types_path, :class => 'btn btn-outline-secondary' %>
<% end %>
