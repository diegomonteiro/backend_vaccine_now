<p id="notice"><%= notice %></p>

<h1>Vaccination Point Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Cod</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vaccination_point_types.each do |vaccination_point_type| %>
      <tr>
        <td><%= vaccination_point_type.name %></td>
        <td><%= vaccination_point_type.cod %></td>
        <td><%= link_to 'Show', vaccination_point_type %></td>
        <td><%= link_to 'Edit', edit_vaccination_point_type_path(vaccination_point_type) %></td>
        <td><%= link_to 'Destroy', vaccination_point_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vaccination Point Type', new_vaccination_point_type_path %>
