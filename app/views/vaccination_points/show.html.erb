<%- model_class = VaccinationPoint -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:vaccination_point_type_id) %>:</strong></dt>
  <dd><%= @vaccination_point.vaccination_point_type_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:name) %>:</strong></dt>
  <dd><%= @vaccination_point.name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:address) %>:</strong></dt>
  <dd><%= @vaccination_point.address %></dd>
  <dt><strong><%= model_class.human_attribute_name(:latitude) %>:</strong></dt>
  <dd><%= @vaccination_point.latitude %></dd>
  <dt><strong><%= model_class.human_attribute_name(:longitude) %>:</strong></dt>
  <dd><%= @vaccination_point.longitude %></dd>
  <dt><strong><%= model_class.human_attribute_name(:contact_number) %>:</strong></dt>
  <dd><%= @vaccination_point.contact_number %></dd>
  <dt><strong><%= model_class.human_attribute_name(:days_work) %>:</strong></dt>
  <dd><%= @vaccination_point.days_work %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              vaccination_points_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_vaccination_point_path(@vaccination_point), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              vaccination_point_path(@vaccination_point),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
