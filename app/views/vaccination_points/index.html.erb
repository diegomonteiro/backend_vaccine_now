<p id="notice"><%= notice %></p>

<h1>Vaccination Points</h1>

<table>
  <thead>
    <tr>
      <th>Vaccination point type</th>
      <th>Name</th>
      <th>Address</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Contact number</th>
      <th>Days work</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vaccination_points.each do |vaccination_point| %>
      <tr>
        <td><%= vaccination_point.vaccination_point_type_id %></td>
        <td><%= vaccination_point.name %></td>
        <td><%= vaccination_point.address %></td>
        <td><%= vaccination_point.latitude %></td>
        <td><%= vaccination_point.longitude %></td>
        <td><%= vaccination_point.contact_number %></td>
        <td><%= vaccination_point.days_work %></td>
        <td><%= link_to 'Show', vaccination_point %></td>
        <td><%= link_to 'Edit', edit_vaccination_point_path(vaccination_point) %></td>
        <td><%= link_to 'Destroy', vaccination_point, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vaccination Point', new_vaccination_point_path %>
