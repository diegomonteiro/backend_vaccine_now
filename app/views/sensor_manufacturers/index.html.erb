<p id="notice"><%= notice %></p>

<h1>Sensor Manufacturers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Cod</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sensor_manufacturers.each do |sensor_manufacturer| %>
      <tr>
        <td><%= sensor_manufacturer.name %></td>
        <td><%= sensor_manufacturer.cod %></td>
        <td><%= link_to 'Show', sensor_manufacturer %></td>
        <td><%= link_to 'Edit', edit_sensor_manufacturer_path(sensor_manufacturer) %></td>
        <td><%= link_to 'Destroy', sensor_manufacturer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sensor Manufacturer', new_sensor_manufacturer_path %>
