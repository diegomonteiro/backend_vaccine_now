<p id="notice"><%= notice %></p>

<h1>Sensor Models</h1>

<table>
  <thead>
    <tr>
      <th>Cod</th>
      <th>Name</th>
      <th>Sensor manufacturer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sensor_models.each do |sensor_model| %>
      <tr>
        <td><%= sensor_model.cod %></td>
        <td><%= sensor_model.name %></td>
        <td><%= sensor_model.sensor_manufacturer_id %></td>
        <td><%= link_to 'Show', sensor_model %></td>
        <td><%= link_to 'Edit', edit_sensor_model_path(sensor_model) %></td>
        <td><%= link_to 'Destroy', sensor_model, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sensor Model', new_sensor_model_path %>
